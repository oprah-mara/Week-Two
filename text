  function compareNumber(){
        var num1 = 10;
        var num2 = 20;

        var resultElement = document.getElementById("result");

        if (num1 > num2){
            resultElement.innerHTML = num1 + " is greater than " + num2;
        }
        else if (num1 < num2){
            resultElement.innerHTML = num1 + " is less than " + num2;
        }
        else{
            resultElement.innerHTML = num1 + " is equal to " + num2;
        }
        }
    function processNumbers(){
        let numbers = [1, 2, 3, 4, 5];
        let result = " ";

        for (let i = 0; i < numbers .length; i++) {
            result += 'Numbers: ' + numbers[i] + '<br>';

        }
        document.getElementById('output').innerHTML = result;
    }
  


